{
    "name": "spoker-v2-root",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "build:dev": "docker compose -f docker-compose.dev.yml up --build",
        "docker:dev": "docker compose -f docker-compose.dev.yml up",
        "build:prod": "docker compose -f docker-compose.prod.yml up --build",
        "docker:prod": "docker compose -f docker-compose.prod.yml up",
        "docker:test": "echo \"Error: no test specified\" && exit 1",
        "test": "npm run test:backend && npm run test:frontend",
        "test:backend": "npm run test --prefix=backend",
        "test:frontend": "npm run test --prefix=frontend",
        "swagger:gen": "node scripts/generate-types.js"
    },
    "devDependencies": {
        "openapi-typescript": "*"
    }
}
